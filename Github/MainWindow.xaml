        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnGenerate" Content="Button" HorizontalAlignment="Left" Height="38" Margin="110,167,0,0" VerticalAlignment="Top" Width="97" Click="Button_Click"/>
        <Label x:Name="lblResult" Content="Label" HorizontalAlignment="Left" Height="38" Margin="110,236,0,0" VerticalAlignment="Top" Width="97"/>
